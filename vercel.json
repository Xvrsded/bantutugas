{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.9.0"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(favicon.ico|robots.txt|logo.svg|payment-logos.html)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*\\.(css|js|mjs|map|png|jpg|jpeg|gif|svg|ico|webp|txt|xml|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
